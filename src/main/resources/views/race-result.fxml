<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.TableView?>
<?import frontend.HomeButton?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.controllers.RaceController">
<Pane id="race-result" maxHeight="1080" maxWidth="1920" minHeight="1080" minWidth="1920"  stylesheets="@style.css" >

    <!-- play background video-->
    <MediaView fx:id="mediaView">
    </MediaView>

    <Pane fx:id="tableBox" layoutX="150" layoutY="65" prefHeight="990" prefWidth="1000" styleClass="block">
        <TableView layoutX="20" layoutY="20" prefHeight="970" prefWidth="975" maxWidth="975" fx:id="resultsTable"/>
    </Pane>

    <HomeButton styleClass="button-xl" layoutY="935" layoutX="1250" fx:id="continueButton" text="Continue"/>
</Pane>
</AnchorPane>